{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "self assessment request",
  "type": "object",
  "properties": {
    "saReturns": {
      "description": "The self assessment returns",
      "type": "array",
      "items": {
          "additionalProperties": false,
          "id": "/properties/saReturns/items",
          "properties": {
            "selfEmploymentStartDate": {
              "id": "/properties/saReturns/items/selfEmploymentStartDate",
              "type": "string",
              "format": "date",
              "description": "The self employment start date",
              "example": "2015-01-28"
            },
            "saReceivedDate": {
              "id": "/properties/saReturns/items/saReceivedDate",
              "type": "string",
              "format": "date",
              "description": "The date on which the self assessment was submitted",
              "example": "2016-01-01"
            },
            "selfAssessmentIncome": {
              "id": "/properties/saReturns/items/selfAssessmentIncome",
              "type": "number",
              "description": "The total income declared in self assessment",
              "example": "1233.33"
            },
            "employmentsIncome": {
              "id": "/properties/saReturns/items/employmentsIncome",
              "type": "number",
              "description": "The total employments income obtained during the tax year",
              "example": "13567.77"
            },
            "selfEmploymentProfit": {
              "id": "/properties/saReturns/items/selfEmploymentProfit",
              "type": "number",
              "description": "The profit obtained for the self employment",
              "example": "1233.33"
            }
          },
          "required": ["saReceivedDate"],
          "type": "object"
      }
    }
  },
  "required": ["saReturns"]
}
